                            "description": "TensorFlow mnist image relying on Kubernetes 1.7 or Openshift 3.7",
                            "iconClass": "icon-processserver",
                            "version": "latest",
                            "sampleRepo": "https://github.com/startxfr/docker-images-example-tensorflow.git"
